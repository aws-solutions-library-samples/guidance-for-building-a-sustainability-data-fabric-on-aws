{
	"name": "usepa:mobile_combustion_ch4_and_n2O_for_on_road_gasoline_vehicles:impacts",
	"description": "Imports USEPA Mobile Combustion CH4 and N2O for On-Road Gasoline Vehicles (table 3) GHG impact factors.",
	"type": "impacts",
	"attributes": {
		"type": "integration"
	},
	"tags": {
		"source": "usepa"
	},
	"connectorConfig": {
		"input": [
			{
				"name": "sif-csv-pipeline-input-connector"
			}
		]
	},
	"transformer": {
		"transforms": [
			{
