				"outputs": [
					{
						"description": "Activity name.",
						"index": 0,
						"key": "activity:name",
						"type": "string"
					}
				]
			},
			{
				"index": 1,
				"formula": "CONCAT('USEPA Mobile Combustion CH4 and N2O for On-Road Gasoline Vehicles emission factors for ', :Vehicle Type, ' (', :Model Year, ').')",
				"outputs": [
					{
						"description": "Activity description.",
						"index": 0,
						"key": "activity:description",
						"type": "string"
					}
				]
			},
			{
				"index": 2,
				"formula": "'US EPA'",
				"outputs": [
					{
						"description": "Activity provider tag.",
						"index": 0,
						"key": "activity:tag:provider",
						"type": "string"
					}
				]
			},
			{
				"index": 3,
				"formula": "'Mobile Combustion CH4 and N2O for On-Road Gasoline Vehicles'",
				"outputs": [
					{
						"description": "Activity dataset tag.",
						"index": 0,
						"key": "activity:tag:dataset",
						"type": "string"
					}
				]
			},
			{
				"index": 4,
				"formula": ":Vehicle Type",
				"outputs": [
					{
						"description": "Activity vehicle type tag.",
