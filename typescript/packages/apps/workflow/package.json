		"@aws-sdk/client-lambda": "3.370.0",
		"@aws-sdk/client-secrets-manager": "3.370.0",
		"@aws-sdk/client-identitystore": "3.515.0",
		"@aws-sdk/client-datazone": "3.515.0",
		"@aws-sdk/client-s3": "3.370.0",
		"@aws-sdk/s3-request-presigner": "3.370.0",
		"@aws-sdk/types": "3.370.0",
		"@aws-sdk/smithy-client": "3.374.0",
		"aws-lambda": "1.0.7",
		"@fastify/awilix": "3.2.0",
		"@fastify/aws-lambda": "3.3.0",
		"@fastify/cors": "8.4.0",
		"@fastify/env": "4.2.0",
		"@fastify/sensible": "5.3.0",
		"@fastify/swagger": "8.12.0",
		"@fastify/type-provider-typebox": "3.5.0",
		"@sinclair/typebox": "0.31.17",
		"awilix": "9.0.0",
		"fastify-plugin": "4.5.1",
		"fastify": "4.24.2",
		"ulid": "2.3.0",
		"p-limit": "4.0.0",
		"aws-xray-sdk": "3.5.0",
		"axios": "1.6.7",
		"jwt-decode": "3.1.2",
		"merge": "2.1.1",
		"@df-sustainability/clients": "workspace:*",
		"@rushstack/eslint-config": "~3.6.8"
